<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:motion="http://schemas.android.com/apk/res-auto">
    
    <Transition
            motion:constraintSetStart="@id/tg_cs_hidden"
            motion:constraintSetEnd="@id/tg_cs_score"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_hidden"
            motion:constraintSetEnd="@id/tg_cs_search"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_hidden"
            motion:constraintSetEnd="@id/tg_cs_searching"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_score"
            motion:constraintSetEnd="@id/tg_cs_hidden"
            motion:duration="200"
            motion:motionInterpolator="linear"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_score"
            motion:constraintSetEnd="@id/tg_cs_search"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_score"
            motion:constraintSetEnd="@id/tg_cs_searching"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_search"
            motion:constraintSetEnd="@id/tg_cs_hidden"
            motion:duration="200"
            motion:motionInterpolator="linear"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_search"
            motion:constraintSetEnd="@id/tg_cs_score"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_search"
            motion:constraintSetEnd="@id/tg_cs_searching"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_searching"
            motion:constraintSetEnd="@id/tg_cs_search"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_searching"
            motion:constraintSetEnd="@id/tg_cs_hidden"
            motion:duration="200"
            motion:motionInterpolator="linear"
    />
    <Transition
            motion:constraintSetStart="@id/tg_cs_searching"
            motion:constraintSetEnd="@id/tg_cs_score"
            motion:duration="200"
            motion:motionInterpolator="easeOut"
    />
    
    <ConstraintSet android:id="@+id/tg_cs_score">
        
        <Constraint
                android:id="@+id/tg_root"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:layout_marginEnd="0dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="20dp"
                android:elevation="10dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintEnd_toEndOf="@id/tg_scorepadder"
        >
            <CustomAttribute
                    motion:attributeName="radius"
                    motion:customDimension="20dp"/>
        
        </Constraint>
        
        <Constraint
                android:id="@+id/tg_scrim"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0"
        />
    
    </ConstraintSet>
    
    <ConstraintSet android:id="@+id/tg_cs_search">
        
        <Constraint
                android:id="@+id/tg_root"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:elevation="10dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
        >
            <CustomAttribute
                    motion:attributeName="radius"
                    motion:customDimension="20dp"/>
        
        </Constraint>
        
        <Constraint
                android:id="@+id/tg_scrim"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0"
        />
    
    </ConstraintSet>
    
    <ConstraintSet android:id="@+id/tg_cs_searching">
        
        <Constraint
                android:id="@+id/tg_root"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:elevation="10dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
        >
            <CustomAttribute
                    motion:attributeName="radius"
                    motion:customDimension="10dp"/>
        
        </Constraint>
        
        <Constraint
                android:id="@+id/tg_scrim"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0.5"
        />
    
    </ConstraintSet>
    
    <ConstraintSet android:id="@+id/tg_cs_hidden">
        
        <Constraint
                android:id="@+id/tg_root"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="0dp"
                android:layout_marginStart="0dp"
                android:elevation="10dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintStart_toEndOf="parent"
        >
            <CustomAttribute
                    motion:attributeName="radius"
                    motion:customDimension="20dp"/>
        
        </Constraint>
        
        <Constraint
                android:id="@+id/tg_scrim"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:alpha="0"
        />
    
    </ConstraintSet>

</MotionScene>
